@model IEnumerable<tp7.Models.Intervention>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DateIntervention)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodeDosNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateIntervention)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodeDosNavigation.CodeDossier)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CodeIntervention">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CodeIntervention">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CodeIntervention">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
